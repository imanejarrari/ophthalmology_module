<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
     
        
        <record id="internal_communication_channel" model="im_livechat.channel">
            <field name="name">Ophthalmology Internal Communication</field>
            <field name="button_text">Chat with Team</field>
            <field name="default_message">Hello! How can I help you?</field>
            <field name="channel_type">internal</field>
        </record>
        
        <function model="im_livechat.channel" name="add_ophthalmology_users">
            <value eval="ref('internal_communication_channel')"/>
        </function>
    </data>
</odoo>
